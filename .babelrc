{
  "presets": ["@babel/preset-env"],
  "sourceMaps": "inline",
  "retainLines": true,
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx",
          ".android.js",
          ".android.tsx",
          ".ios.js",
          ".ios.tsx"
        ],
        "root": ["."]
      }
    ]
  ],
  "parserOpts": {
    "allowReturnOutsideFunction": true
  },
  "ignore": ["node_modules"]
}